{
  "name": "antd-admin-template",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview --port 4173",
    "lint": "eslint src/** --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix src/** --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint"
    }
  },
  "lint-staged": {
    "*.js(?:on)?": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn lint:fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@tanstack/react-query": "4.2.3",
    "antd": "5.0.3",
    "axios": "1.2.0",
    "dayjs": "1.11.6",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "styled-components": "5.3.6",
    "valtio": "1.7.6"
  },
  "devDependencies": {
    "@testing-library/react": "12.1.2",
    "@types/node": "14.18.9",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@vitejs/plugin-react": "2.1.0",
    "@types/styled-components": "5.1.26",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-define-config": "1.11.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "vite": "3.2.2",
    "vite-plugin-windicss": "1.8.8",
    "vitest": "0.16.0",
    "windicss": "3.5.6"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
